#: de/cubeisland/engine/backpack/BackpackCommands.java:99
msgid ""
"<name> [user] [-global]|[-single] [-blockinput] [w <world>] [p <pages>] [s "
"<size>]"
msgstr "<nom> [joueur] [-global] [-single] [-blockinput] [w <monde>] [p <page>] [s <taille>]"

#: de/cubeisland/engine/backpack/BackpackCommands.java:53
msgid "<name> [user] [w <world>]"
msgstr "<nom> [joueur] [w <monde>]"

#: de/cubeisland/engine/backpack/BackpackCommands.java:203
msgid ""
"<name> [user] [w <world>] <item <item>[:data]> [name <name>] [lore "
"<loreline>[,<loreline>]] [ench <enchs...>] [amount <amount>]"
msgstr "<nom> [joueur] [w <monde>] <objet> <objet>[:data]> [name <nom>] [lore <ligne>[,<ligne>]] [ench <enchantement...>] [amount <quantité>]"

#: de/cubeisland/engine/backpack/BackpackCommands.java:152
msgid ""
"<name> [user] [w <world>] [pages <pages>] [s <size>] [blockinput "
"<true|false>]"
msgstr "<nom> [joueur] [w <monde>] [pages <page>] [s <taille>] [blockinput <true|false>]"

#: de/cubeisland/engine/backpack/BackpackCommands.java:202
msgid "Puts items into a backpack"
msgstr "Dépose des objets dans un sac à dos"

#: de/cubeisland/engine/backpack/BackpackCommands.java:98
msgid "creates a new backpack"
msgstr "Crée un nouveau sac à dos"

#: de/cubeisland/engine/backpack/BackpackCommands.java:151
msgid "modifies a backpack"
msgstr "Change un sac à dos"

#: de/cubeisland/engine/backpack/BackpackCommands.java:53
msgid "opens a backpack"
msgstr "Ouvre un sac à dos"

#: de/cubeisland/engine/backpack/BackpackManager.java:92
