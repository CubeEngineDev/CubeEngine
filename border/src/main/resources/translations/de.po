#: de/cubeisland/engine/border/BorderCommands.java:108
msgid "<world>"
msgstr "<world>"

#: de/cubeisland/engine/border/BorderCommands.java:108
msgid "Generates the chunks located in the border"
msgstr "Generiert die Chunks die in der Begrenzung liegen."

#: de/cubeisland/engine/border/BorderCommands.java:57
msgid "Sets the center of the border"
msgstr "Legt den Mittelpunkt der Begrenzung fest"

#: de/cubeisland/engine/border/BorderCommands.java:57
msgid "[<chunkX> <chunkZ>]|[-spawn] [in <world>]"
msgstr "[<chunkX> <chunkZ>]|[-spawn] [in <Welt>]"

#: de/cubeisland/engine/border/BorderCommands.java:181
msgid "Added {amount} chunks to generate in {world}"
msgstr "Es wurden {amount} zu erzeugende Chunks in {world} hinzugefügt"

#: de/cubeisland/engine/border/BorderListener.java:101
msgid ""
"As you reach the border the world turns on itself and you appear on the "
"other side of the world!"
msgstr "Wenn Du die Begrenzung erreichst, wirst Du am anderen Ende Welt wieder erscheinen!"

#: de/cubeisland/engine/border/BorderCommands.java:224
msgid "Available Memory getting low! Pausing Chunk Generation"
msgstr ""

#: de/cubeisland/engine/border/BorderCommands.java:80
msgid "Center for Border in {world} set to world spawn!"
msgstr ""

#: de/cubeisland/engine/border/BorderCommands.java:104
msgid "Center for Border in {world} set!"
msgstr "Mittelpunkt für die Begrenzung in {world} gesetzt!"

#: de/cubeisland/engine/border/BorderCommands.java:269
msgid "Chunk generation completed! Generated {amount} chunks"
msgstr ""

#: de/cubeisland/engine/border/BorderCommands.java:113
msgid "Chunk generation is already running!"
msgstr "Chunkaufbau wird bereits ausgeführt!"

#: de/cubeisland/engine/border/BorderCommands.java:256
msgid ""
"Chunk generation is at {integer#percent}% ({amount#done}/{amount#total})"
msgstr ""

#: de/cubeisland/engine/border/BorderCommands.java:89
msgid "Invalid Chunk coordinates!"
msgstr ""

#: de/cubeisland/engine/border/BorderListener.java:162
msgid "This portal would teleport you behind the border!"
msgstr "Dieses Portal würde dich hinter die Begrenzung teleportieren!"

#: de/cubeisland/engine/border/BorderCommands.java:131
msgid "World {input} not found!"
msgstr "Welt {input} nicht gefunden!"

#: de/cubeisland/engine/border/BorderListener.java:114
msgid "You are near the world border! You might want to turn around."
msgstr ""

#: de/cubeisland/engine/border/BorderListener.java:88
msgid "You cannot teleport outside the border!"
msgstr "Du kannst nicht an eine Stelle außerhalb der Begrenzung teleportiert werden!"

#: de/cubeisland/engine/border/BorderCommands.java:69
msgid "You need to specify a world!"
msgstr "Du musst eine Welt angeben!"

#: de/cubeisland/engine/border/BorderCommands.java:100
msgid "You need to specify the chunk coordinates or use the -spawn flag"
msgstr ""

#: de/cubeisland/engine/border/BorderListener.java:108
msgid "You've reached the border!"
msgstr "Du hast die Begrenzung erreicht!"
