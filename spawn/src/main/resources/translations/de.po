#: de/cubeisland/engine/spawn/SpawnCommands.java:60
msgid "Changes the respawnpoint"
msgstr "Verschiebt den Respawn-Punkt"

#: de/cubeisland/engine/spawn/SpawnCommands.java:140
msgid "Teleport directly to the worlds spawn."
msgstr "Teleport direkt zum Welt-Spawn."

#: de/cubeisland/engine/spawn/SpawnCommands.java:61
msgid "[<role>|global] [<x> <y> <z>] [world]"
msgstr "[<Rolle>|global] [<x> <y> <z>] [world]"

#: de/cubeisland/engine/spawn/SpawnCommands.java:141
msgid "[player]|* [world <world>] [role <role>]"
msgstr "[Spieler]|* [world <Welt>] [role <Rolle>]"

#: de/cubeisland/engine/spawn/SpawnCommands.java:96
msgid "Coordinates are invalid!"
msgstr "Die Koordinaten sind ungültig!"

#: de/cubeisland/engine/spawn/SpawnCommands.java:114
#: de/cubeisland/engine/spawn/SpawnCommands.java:180
msgid "Could not find the role {input} in {world}!"
msgstr "Rolle {input} wurde in {world} nicht gefunden!"

#: de/cubeisland/engine/spawn/SpawnCommands.java:66
msgid "If not used ingame you have to specify a world and coordinates!"
msgstr "Wenn nicht im Spiel benutzt, müssen eine Welt und die Koordinaten angeben werden!"

#: de/cubeisland/engine/spawn/SpawnCommands.java:165
msgid "Show this error to an administrator!"
msgstr "Zeige diesen Fehler einem Administrator!"

#: de/cubeisland/engine/spawn/SpawnCommands.java:274
msgid "Teleported {1:user} to the spawn of {world}"
msgid_plural "Teleported {2:integer#amount} users to the spawn of {world}"
msgstr[0] "Es wurde {1:user} zum Spawn von {world} teleportiert."
msgstr[1] "Er wurden {2:integer#amount} Spieler zum Spawn von {world} teleportiert."

#: de/cubeisland/engine/spawn/SpawnCommands.java:281
msgid "Teleported {2:user} to the spawn of the role {name#role} in {world}"
msgid_plural ""
"Teleported {3:integer#amount} users to the spawn of the role {name#role} in "
"{world}"
msgstr[0] "Es wurde {2:user} zum Spawn der Rolle {name#role} in {world} teleportiert."
msgstr[1] "Es wurden {3:integer#amount} Spieler zum Spawn der Rolle {name#role} in {world} teleportiert."

#: de/cubeisland/engine/spawn/SpawnCommands.java:186
msgid "The role {name} in {world} has no spawn point!"
msgstr "Die Rolle {name} hat in {world} keinen Spawn-Punkt!"

#: de/cubeisland/engine/spawn/SpawnCommands.java:123
msgid "The spawn in {world} is now set to {vector}"
msgstr "Der Spawn in {world} liegt nun bei {vector}"

#: de/cubeisland/engine/spawn/SpawnCommands.java:67
msgid "Use {text:global} instead of the role name to set the default spawn."
msgstr "Benutzer {text:global} statt dem Rollennamen um den Standard-Spawn-Punkt zu setzen."

#: de/cubeisland/engine/spawn/SpawnCommands.java:238
msgid "User {user} not found!"
msgstr "Benutzer {user} nicht gefunden!"

#: de/cubeisland/engine/spawn/SpawnCommands.java:76
msgid "World {input} not found"
msgstr "Welt {input} nicht gefunden"

#: de/cubeisland/engine/spawn/SpawnCommands.java:158
msgid "World {input} not found!"
msgstr "Welt {input} nicht gefunden!"

#: de/cubeisland/engine/spawn/SpawnCommands.java:217
msgid "You are not allowed to spawn everyone!"
msgstr "Du darfst nicht alle spawnen!"

#: de/cubeisland/engine/spawn/SpawnCommands.java:248
msgid "You are not allowed to spawn {user}!"
msgstr "Du darfst {user} nicht spawnen!"

#: de/cubeisland/engine/spawn/SpawnCommands.java:322
msgid "You are now standing at the spawn of {name#role} in {world}!"
msgstr "Du stehst jetzt am Spawn von {name#role} in {world}!"

#: de/cubeisland/engine/spawn/SpawnCommands.java:243
msgid "You cannot teleport an offline player to spawn!"
msgstr "Du kannst keinen Offline-Spieler an den Spawn teleportieren!"

#: de/cubeisland/engine/spawn/SpawnCommands.java:149
msgid "{text:Pro Tip}: Teleport does not work IRL!"
msgstr "{text:Pro Tip}: Teleportieren klappt noch nicht IRL!"

#: de/cubeisland/engine/spawn/SpawnCommands.java:192
msgid ""
"Invalid spawn location for the role {name}! Please check your role "
"configuration!"
msgstr ""

#: de/cubeisland/engine/spawn/SpawnCommands.java:313
msgid ""
"Invalid spawn location for the role! Please check your role configuration!"
msgstr ""

#: de/cubeisland/engine/spawn/SpawnCommands.java:164
msgid "Unknown main world configured!"
msgstr ""

#: de/cubeisland/engine/spawn/SpawnCommands.java:306
msgid "You are now standing at the spawn in {world}!"
msgstr ""

#: de/cubeisland/engine/spawn/SpawnCommands.java:317
msgid "You are now standing at your role's spawn!"
msgstr ""
